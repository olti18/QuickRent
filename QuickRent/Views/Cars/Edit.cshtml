@using QuickRent.Models;
@model QuickRent.Models.Car
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container shadow p-5">
    <div class="row pb-2">
        <h2>Edit a Breed</h2>
    </div>

    <form method="post" asp-action="Edit">
        <div asp-validation-summary="All"></div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="Fuel">Employee Name</label>
                <input type="text" class="form-control mb-3" asp-for="Fuel">
                <span asp-validation-for="Fuel" class=" alert-danger"></span>
            </div>
            <div class="form-group col-md-6">
                <label asp-for="Description">Designation</label>
                <input type="text" class="form-control mb-3" asp-for="Description">
                <span asp-validation-for="Description" class=" alert-danger"></span>
            </div>
        </div>



        <button type="submit" class="btn btn-lg btn-primary p-2"><i class="bi bi-file-plus-fill"></i>Update</button>
        <a asp-controller="Cars" asp-action="Index" class="btn btn-lg btn-warning p-2">Back To List</a>
    </form>

</div>
